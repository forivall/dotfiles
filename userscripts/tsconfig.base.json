{
  "compileOnSave": true,
  "compilerOptions": {
    "typeRoots": [],
    "target": "es2017",
    "module": "es6",
    "lib": [
      "es2017",
      "dom",
      "dom.iterable"
    ],
    "declaration": false,
    "declarationMap": false,
    "sourceMap": true,
    "removeComments": false,
    "alwaysStrict": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "outDir": "tsc-outdir-not-set"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
