  "autocomplete-flow":
    pathToFlowExecutable: "/usr/local/bin/flow"
  "autocomplete-plus":
    autoActivationDelay: 150
    backspaceTriggersAutocomplete: true
    confirmCompletion: "tab"
    includeCompletionsFromAllBuffers: true
    maxVisibleSuggestions: 20
    useAlternateScoring: false
  "base16-syntax":
    scheme: "Tomorrow"
  core:
    autoHideMenuBar: true
    disabledPackages: [
      "background-tips"
      "block-comment"
      "es-identifier-highlight"
      "fold-comments"
      "fold-functions"
      "fold-search-results"
      "git-gui"
      "git-wip"
      "github-notifications"
      "gtk-dark-theme"
      "highlight-selected"
      "html2jade-plus"
      "imdone-atom-github"
      "imdone-atom"
      "linter-flake8"
      "linter-jshint"
      "linter-nplint"
      "linter-scss-lint"
      "linter-stylint"
      "omnisharp-atom"
      "open-git-modified-files"
      "pane-by-type"
      "preview-plus"
      "project-find-navigation"
      "promote-folder-to-project"
      "raml"
      "remember-file-positions"
      "require-opener"
      "run-in-terminal"
      "string-case"
      "symbols-tree-view"
      "tablr"
      "term3"
      "tree-view-breadcrumb"
      "welcome"
      "wrap-guide"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      ".imdone"
      "tags"
      "npm-debug.log"
      "$RECYCLE.BIN"
      "*.localized"
    ]
    packagesWithKeymapsDisabled: [
      "sort-lines"
      "emmet"
      "imdone-atom"
      "es-identifier-highlight"
    ]
    themes: [
      "native-ui"
      "today"
    ]
  "cursor-history":
    flashColor: "success"
    flashOnLand: true
    flashType: "word"
  "custom-invisibles":
    crActive: false
    eolActive: false
    spaceActive: false
  docblockr:
    indentation_spaces: 2
    indentation_spaces_same_para: 0
    simple_mode: true
  editor:
    backUpBeforeSaving: true
    fontFamily: "Fantasque Sans Mono"
    invisibles:
      cr: " "
      eol: " "
      space: " "
    lineHeight: 1.3
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrapAtPreferredLineLength: true
  "es-identifier-highlight":
    lightTheme: false
  "exception-reporting":
