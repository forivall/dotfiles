"*":
  "activate-power-mode":
    autoToggle: false
    screenShake:
      enabled: false
  "adwaita-pro-ui":
    fontFamily: "Oxygen-Sans"
    fontSize: 13
    iconTheme: "Octicons"
  "atom-beautify":
    general:
      _analyticsUserId: "fb921142-84d5-4639-8d26-e13dc763a02c"
  "atom-capyba-ui":
    layoutMode: "Compact"
  "atom-mac-terminal":
    terminal: "iTerm2"
  "atom-material-ui":
    accentColor: "Blue"
    compactTreeView: true
    depth: true
    disableAnimations: true
    fontSize: "Big"
    panelContrast: true
    showTabIcons: true
  "atom-terminal":
    app: "roxterm"
    args: "--tab"
  "atom-ternjs":
