"*":
  "activate-power-mode":
    autoToggle: false
    screenShake:
      enabled: false
  "adwaita-pro-ui":
    fontFamily: "Droid Sans"
    fontSize: 13
    iconTheme: "Octicons"
  "atom-beautify":
    general:
      _analyticsUserId: "fb921142-84d5-4639-8d26-e13dc763a02c"
  "atom-capyba-ui":
    layoutMode: "Compact"
  "atom-mac-terminal":
    terminal: "iTerm2"
  "atom-material-ui":
    accentColor: "Blue"
    compactTreeView: true
    depth: true
    disableAnimations: true
    fontSize: "Big"
    panelContrast: true
    showTabIcons: true
  "atom-terminal":
    app: "roxterm"
    args: "--tab"
  "atom-ternjs":
    lint: false
  "autocomplete-plus":
    confirmCompletion: "tab"
    includeCompletionsFromAllBuffers: true
  "base16-syntax":
    scheme: "Eighties"
  core:
    autoHideMenuBar: true
    destroyEmptyPanes: false
    disabledPackages: [
      "atom-beautify"
      "background-tips"
      "block-comment"
      "custom-invisibles"
      "hard-wrap"
      "linter-flake8"
      "linter-jshint"
      "linter-scss-lint"
      "linter-stylint"
      "pane-by-type"
      "pigments"
      "preview-plus"
      "tree-view-breadcrumb"
      "welcome"
      "wrap-guide"
      "run-in-terminal"
      "git-wip"
      "autocomplete-flow"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      ".imdone"
      "tags"
      "npm-debug.log"
    ]
    packagesWithKeymapsDisabled: [
      "sort-lines"
      "emmet"
      "imdone-atom"
    ]
    themes: [
      "adwaita-pro-ui"
      "today"
    ]
  "cursor-history":
    flashColor: "success"
    flashOnLand: true
    flashType: "word"
  "custom-invisibles":
    crActive: false
    eolActive: false
    spaceActive: false
  docblockr:
    indentation_spaces: 2
    indentation_spaces_same_para: 0
    simple_mode: true
  editor:
    backUpBeforeSaving: true
    fontFamily: "Fantasque Sans Mono"
    invisibles:
      cr: " "
      eol: " "
      space: " "
    lineHeight: 1.2
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
  "es-identifier-highlight":
    lightTheme: true
  "exception-reporting":
    userId: "d669d80a-cb48-445f-92bf-5ae1bb0d0696"
  "file-icons":
    coloured: false
    onChanges: true
  "file-types":
    el: "source.perl"
  "fold-functions": {}
  "fuzzy-finder":
    preserveLastSearch: true
    searchAllPanes: true
  "git-plus": {}
  "git-projects": {}
  "git-wip":
    notify: false
    warn: false
  "github-notifications":
    notificationAuthToken: " a77986444ecd25d526561a4606b0be8318c6cca7"
  "highlight-selected":
    lightTheme: true
    onlyHighlightWholeWords: true
  "ide-flow":
    flowPath: "/usr/bin/flow"
  "indentation-indicator":
    indicatorPosition: "right"
    spaceAfterColon: true
  "js-autoimport": {}
  "language-babel": {}
  linter:
    ignoredLinterErrors: [
      "scss-lint"
    ]
    lintOnChange: false
    lintOnFly: false
    showErrorTabLine: true
    subtleLinterErrors: [
      "shellcheck"
    ]
    underlineIssues: false
  "linter-clang": {}
  "linter-eslint": {}
  "linter-jshint":
    disableWhenNoJshintrcFileInPath: true
  "linter-scss-lint":
    executablePath: "/usr/bin/scss-lint"
  "linter-stylint":
    stylintExecutablePath: "/home/forivall/.nvm/versions/node/v0.12.1/bin/stylint"
  "markdown-table-formatter": {}
  mdattachment:
    commonDirName: "."
    oneDirFolAllBuffers: true
  minimap:
    charHeight: 1.5
    charWidth: 0.7
    displayPluginsControls: false
    interline: 0.4
    scrollAnimation: true
    scrollAnimationDuration: 100
    textOpacity: 0.5
  "minimap-codeglance": {}
  "one-light-ui":
    fontSize: 13
  pigments:
    ignoredNames: [
      "*"
    ]
    sortPaletteColors: "by color"
  "preview-plus": {}
  "project-manager": {}
  "project-view": {}
  "recent-files-fuzzy-finder": {}
  "run-in-terminal":
    save_before_launch: false
    terminal: "ATOM_HOME= CHROME_DESKTOP= GOOGLE_API_KEY= NODE_ENV= NODE_PATH= PYTHON= mate-terminal --tab"
  "save-session":
    dataSaveFolder: "/home/forivall/.atom/packages/save-session/projects"
    skipSavePrompt: false
  "slim-dark-ui":
    layoutMode: "Compact"
  "status-bar": {}
  "svg-preview":
    openPreviewAutomatically: true
  tabs:
    showIcons: true
    usePreviewTabs: true
  term3: {}
  "test-status":
    autorun: false
  "theme-switcher":
    syntaxThemeOne: "tomorrow-ya"
    syntaxThemeTwo: "today"
    uiThemeOne: "adwaita-pro-ui"
    uiThemeTwo: "adwaita-pro-ui"
  "tree-view":
    alwaysOpenExisting: true
    hideIgnoredNames: true
    refreshVcsStatusOnProjectOpen: 5
  "tree-view-breadcrumb":
    displayProjectRoot: true
    displayProjectRootName: true
    keepAttached: true
    keepBreadcrumbVisible: true
    pathStyle: true
  "tree-view-git-projects":
    alphabetizeRoots: false
  welcome:
    showOnStartup: false
  whitespace: {}
  wrap: {}
  "you-complete-me":
    pythonExecutable: "/usr/bin/python2"
